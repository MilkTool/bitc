      <tr lineafter="yes">
	<td/>
	<td/>
	<td/>
	<td/>
	<td>
	  <p>
	    <btypes:TYPE>
	      <const>
		<type/>
	      </const>
	    </btypes:TYPE>
	    otherwise
	  </p>
	</td>
	<td/>
