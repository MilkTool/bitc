 <figure id="hm_solve" fullwidth='yes' latex.placement="htb">
  <font size="small">
    <btypes:TYPE
      xmlns:btypes="http://www.bitc-lang.org/DTD/bitc/0.1/bitc-types.dtd">
      <Hrules rowsep="4pt" colsep="2">
	<tyRule name="Unify">
	  <tyPre>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<Xform name="real" isEntity="yes">
		  <type num="1"/>
		</Xform>
		<Xform name="real" isEntity="yes">
		  <type num="2"/>
		</Xform>
	      </eq>
	    </Sjudge>
	  </tyPre>
	  <tyConc>
	    <UNIFY>
	      <type num="1"/>
	      <type num="2"/>
	      <aSubMap/>
	    </UNIFY>
	  </tyConc>
	</tyRule>
       <tyRule name="U-Refl">
          <tyPre/>
          <tyConc>
            <Sjudge name="u">
              <assume>
                <Empty/>
              </assume>
              <eq>
                <type/>
                <type/>
              </eq>
            </Sjudge>
          </tyConc>
        </tyRule>
	<tyRule name="U-Commut">
	  <tyPre>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<type/>
		<type dash="'"/>
	      </eq>
	    </Sjudge>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<type dash="'"/>
		<type/>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
	<tyRule name="U-Tvar">
	  <tyPre>
	    <neq>
	      <tvar name="alpha"/>
	      <type/>
	    </neq>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<SubMap>
		  <tvar name="alpha"/>
		  <type/>
		</SubMap>
	      </assume>
	      <eq>
		<tvar name="alpha"/>
		<type/>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
	<tyRule name="U-Mut">
	  <tyPre>                
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<type num="1"/>
		<type num="2"/>
	      </eq>
	    </Sjudge>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<mutable>
		  <type num="1"/>
		</mutable>
		<mutable>
		  <type num="2"/>
		</mutable>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
      </Hrules>
      <Hrules rowsep="4pt" colsep="2">
	<tyRule name="U-Fn">
	  <tyPre>                
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>	      
		<type num="1"/>
		<type num="1" dash="'"/>
	      </eq>
	    </Sjudge>
	    <Sjudge name="u">
	      <assume>
		<aSubMap dash="'"/>
	      </assume>
	      <eq>
		<Subst>
		  <aSubMap/>
		  <type num="2"/>
		</Subst>
		<Subst>
		  <aSubMap/>
		  <type num="2" dash="'"/>
		</Subst>
	      </eq>
	    </Sjudge>
	    <models name="acl">
	      <assume/>
	      <compose>
		<aSubMap/>
		<aSubMap dash="'"/>
	      </compose>
	    </models>
	  </tyPre>
	  <tyConc> 
	    <Sjudge name="u">
	      <assume>
		<compose>
		  <aSubMap/>
		  <aSubMap dash="'"/>
		</compose>
	      </assume>
	      <eq>
		<fn>
		  <type num="1"/>
		  <type num="2"/>
		</fn>
		<fn>
		  <type num="1" dash="'"/>
		  <type num="2" dash="'"/>
		</fn>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
	<tyRule name="U-Floor">
	  <tyPre>                
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<type num="1"/>
		<type num="2"/>
	      </eq>
	      <aSubMap/>
	    </Sjudge>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<floor>
		  <type num="1"/>
		</floor>
		<floor>
		  <type num="2"/>
		</floor>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
	<tyRule name="U-Ceil">
	  <tyPre>                
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<type num="1"/>
		<type num="2"/>
	      </eq>
	    </Sjudge>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<ceil>
		  <type num="1"/>
		</ceil>
		<ceil>
		  <type num="2"/>
		</ceil>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
      </Hrules> 
      <Hrules rowsep="4pt" colsep="2">
	<tyRule name="U-Ref">
	  <tyPre>                
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<type num="1"/>
		<type num="2"/>
	      </eq>
	    </Sjudge>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<ref>
		  <type num="1"/>
		</ref>
		<ref>
		  <type num="2"/>
		</ref>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule> 
	<tyRule name="U-Mb-Mb">
	  <tyPre>                
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<inner>
		  <type num="1" dash="'"/>
		</inner>
		<inner>
		  <type num="2" dash="'"/>
		</inner>
	      </eq>
	    </Sjudge>
	    <Sjudge name="u">
	      <assume>
		<aSubMap dash="'"/>
	      </assume>
	      <eq>
		<Subst>
		  <aSubMap/>
		  <type num="1"/>
		</Subst>
		<Subst>
		  <aSubMap/>
		  <type num="2"/>
		</Subst>
	      </eq>
	    </Sjudge>
	    <models name="acl">
	      <assume/>  
	      <compose>
		<aSubMap/>
		<aSubMap dash="'"/>
	      </compose>
	    </models>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<compose>
		  <aSubMap/>
		  <aSubMap dash="'"/>
		</compose>
	      </assume>
	      <eq>
		<mbpair>
		  <type num="1"/>
		  <type num="1" dash="'"/>
		</mbpair>
		<mbpair>
		  <type num="2"/>
		  <type num="2" dash="'"/>
		</mbpair>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
	<tyRule>
	  <tyPre>     
	    <eq>
	      <type/>
	      <mbpair>
		<type num="1"/>
		<type num="2"/>
	      </mbpair>
	    </eq>
	  </tyPre>
	  <tyPre>
	    <eq>
	      <inner>
		<type num="2"/>
	      </inner>
	      <type num="2" dash="'"/>
	    </eq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <inner>
		<type/>
	      </inner>
	      <type num="2" dash="'"/>
	    </eq>
	  </tyConc>
	</tyRule>	
      </Hrules> 
      <Hrules rowsep="4pt" colsep="2">
	<tyRule name="U-Mb-Oth">
	  <tyPre>
	    <neq>
	      <type num="2"/>
	      <mbpair>
		<type num="3"/>
		<type num="3" dash="'"/>
	      </mbpair>
	    </neq>
	    <neq>
	      <type num="2"/>
	      <tvar name="alpha"/>
	    </neq>
	    <Sjudge name="u">
	      <assume>
		<aSubMap/>
	      </assume>
	      <eq>
		<inner>
		  <type num="1" dash="'"/>
		</inner>
		<minz>
		  <type num="2"/>
		</minz>
	      </eq>
	    </Sjudge>
	    <Sjudge name="u">
	      <assume>
		<aSubMap dash="'"/>
	      </assume>
	      <eq>
		<Subst>
		  <aSubMap/>
		  <type num="1"/>
		</Subst>
		<Subst>
		  <aSubMap/>
		  <type num="2"/>
		</Subst>
	      </eq>
	    </Sjudge>
	    <models name="acl">
	      <assume/>
	      <compose>
		<aSubMap/>
		<aSubMap dash="'"/>
	      </compose>
	    </models>
	  </tyPre>
	  <tyConc>
	    <Sjudge name="u">
	      <assume>
		<compose>
		  <aSubMap/>
		  <aSubMap dash="'"/>
		</compose>
	      </assume>
	      <eq>
		<mbpair>
		  <type num="1"/>
		  <type num="1" dash="'"/>
		</mbpair>
		<type num="2"/>
	      </eq>
	    </Sjudge>
	  </tyConc>
	</tyRule>
	<tyRule name="">
	  <tyPre>
	    <neq>
	      <type/>
	      <mbpair>
		<type num="1"/>
		<type num="2"/>
	      </mbpair>
	    </neq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <inner>
		<type/>
	      </inner>
	      <minz>
		<type/>
	      </minz>
	    </eq>
	  </tyConc>
	</tyRule>
      </Hrules> 
      <Hrules rowsep="4pt" colsep="2">
	<tyRule>
	  <tyPre/>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<tvar name="alpha"/>
	      </Xform>
	      <tvar name="alpha"/>
	    </eq>      
	  </tyConc>
	</tyRule>
	<tyRule>
	  <tyPre/>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<bool/>
	      </Xform>
	      <floor>
		<bool/>
	      </floor>
	    </eq>
	  </tyConc>
	</tyRule>
	<tyRule>
	  <tyPre/>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<unit/>
	      </Xform>
	      <floor>
		<unit/>
	      </floor>
	    </eq>
	  </tyConc>
	</tyRule>
	<tyRule>
	  <tyPre>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<type/>
	      </Xform>
	      <type dash="'"/>
	    </eq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<mutable>
		  <type/>
		</mutable>
	      </Xform>
	      <ceil>
		<mutable>
		  <type dash="'"/>
		</mutable>
	      </ceil>
	    </eq>
	  </tyConc>
	</tyRule>
	<tyRule>
	  <tyPre>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<type/>
	      </Xform>
	      <type dash="'"/>
	    </eq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<ref>
		  <type/>
		</ref>
	      </Xform>
	      <floor>
		<ref>
		  <type dash="'"/>
		</ref>
	      </floor>
	    </eq>
	  </tyConc>
	</tyRule>
      </Hrules>
      <Hrules colsep="2">
	<tyRule>
	  <tyPre>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<type num="1"/>
	      </Xform>
	      <type num="1" dash="'"/>
	    </eq>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<type num="2"/>
	      </Xform>
	      <type num="2" dash="'"/>
	    </eq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<fn>
		  <type num="1"/>
		  <type num="2"/>
		</fn>
	      </Xform>
	      <floor>
		<fn>
		  <type num="1" dash="'"/>
		  <type num="2" dash="'"/>
		</fn>
	      </floor>
	    </eq>
	  </tyConc>
	</tyRule>
	<tyRule>
	  <tyPre>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<type num="1"/>
	      </Xform>
	      <type num="1" dash="'"/>
	    </eq>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<type num="2"/>
	      </Xform>
	      <type num="2" dash="'"/>
	    </eq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<mbpair>
		  <type num="1"/>
		  <type num="2"/>
		</mbpair>
	      </Xform>
	      <mbpair>
		<type num="1" dash="'"/>
		<type num="2" dash="'"/>
	      </mbpair>
	    </eq>
	  </tyConc>
	</tyRule>           
	<tyRule>
	  <tyPre>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<minz>
		  <type/>
		</minz>
	      </Xform>
	      <type dash="'"/>
	    </eq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<floor>
		  <type/>
		</floor>
	      </Xform>
	      <floor>
		<type dash="'"/>	      
	      </floor>
	    </eq>
	  </tyConc>
	</tyRule>
	<tyRule>
	  <tyPre>
	    <eq>
	      <Xform name="real" isEntity="yes">	      
		<maxz>
		  <type/>
		</maxz>
	      </Xform>
	      <type dash="'"/>
	    </eq>
	  </tyPre>
	  <tyConc>
	    <eq>
	      <Xform name="real" isEntity="yes">
		<ceil>
		  <type/>
		</ceil>
	      </Xform>
	      <type dash="'"/>
	    </eq>
	  </tyConc>
	</tyRule>
      </Hrules>
