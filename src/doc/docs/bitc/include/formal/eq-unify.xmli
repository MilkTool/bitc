<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE floatingtable PUBLIC "-//EROS Group//DTD OSDoc XML V0.1//EN"
               "http://www.coyotos.org/OSDoc/DTD/osdoc-0.1.dtd" >
<figure id="eq_unify" fullwidth='yes' latex.placement="tbp">
  <font size="small">
    <btypes:TYPE
      xmlns:btypes="http://www.bitc-lang.org/DTD/bitc/0.1/bitc-types.dtd">
      <VEqns>
	<!-- Empty -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <Empty/>
	    </unf>
	  </lhs>
	  <rhs>
	    <Pair>
	      <Empty/>
	      <EmptySubst/>
	    </Pair>
	  </rhs>
	</equation>
	<!-- a <= a -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <tvar name="alpha"/>
		    <tvar name="alpha"/>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf>
	      <aCtset/>
	    </unf>
	  </rhs>
	</equation>
	<!-- unit <= unit -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <unit/>
		    <unit/>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf>
	      <aCtset/>
	    </unf>
	  </rhs>
	</equation>
	<!-- bool <= bool -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <bool/>
		    <bool/>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf>
	      <aCtset/>
	    </unf>
	  </rhs>
	</equation>
	<!-- Mt <= t -->
	<equation sep="4pt">
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <mutable>
		      <type/>
		    </mutable>
		    <type/>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf>
	      <aCtset/>
	    </unf>
	  </rhs>
	</equation>
	<!-- a = t -->
	<equation>	  
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <eq>
		    <tvar name="alpha"/>
		    <type/>
		  </eq>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <Pair>
	      <aCtset num="i"/>
	      <scomp>
		<aSubMap num="a"/>
		<aSubMap num="i"/>
	      </scomp>	    
	    </Pair>	 
	    <eq>
	      <aSubMap num="a"/>
	      <SubMap>
		<tvar name="alpha"/>
		<type/>
	      </SubMap>
	    </eq>
	    <eq>
	      <unf cl="yes">
		<Subst>
		  <aSubMap num="a"/>
		  <aCtset/>
		</Subst>
	      </unf>
	      <Pair>
		<aCtset num="i"/>
		<aSubMap num="i"/>
	      </Pair>
	    </eq>
	  </rhs>
	</equation>
	<!-- k = K -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <eq>
		    <lKind k="var"/>
		    <lKind/>
		  </eq>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <Pair>
	      <aCtset num="i"/>
	      <scomp>
		<aSubMap num="k"/>
		<aSubMap num="i"/>
	      </scomp>	    
	    </Pair>	  	    
	    <eq>
	      <aSubMap num="k"/>
	      <SubMap>
		<lKind k="var"/>
		<lKind/>
	      </SubMap>
	    </eq>
	    <eq>
	      <unf>
		<Subst>
		  <aSubMap num="k"/>
		  <aCtset/>
		</Subst>
	      </unf>
	      <Pair>
		<aCtset num="i"/>
		<aSubMap num="i"/>
	      </Pair>
	    </eq>
	  </rhs>
	</equation>
	<!-- a <= Mt -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <tvar name="alpha"/>
		    <type/>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	    <eq>
	      <type/>
	      <maxz>
		<type/>
	      </maxz>
	    </eq>
	    <notin>
	      <Tsub>
		<type/>
		<tvar name="alpha"/>
	      </Tsub>
	      <aCtset/>
	    </notin>	    
	  </lhs>
	  <rhs>
	    <Pair>
	      <aCtset num="i"/>
	      <scomp>
		<aSubMap num="a"/>
		<aSubMap num="i"/>
	      </scomp>	    
	    </Pair>	  	    
	    <eq>
	      <aSubMap num="a"/>
	      <SubMap>
		<tvar name="alpha"/>
		<type/>
	      </SubMap>
	    </eq>
	    <eq>
	      <unf cl="yes">
		<Subst>
		  <aSubMap num="a"/>
		  <aCtset/>
		</Subst>
	      </unf>
	      <Pair>
		<aCtset num="i"/>
		<aSubMap num="i"/>
	      </Pair>
	    </eq>
	  </rhs>
	</equation>	
	<!-- It <= a -->
	<equation sep="4pt">
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>		    
		    <type/>
		    <tvar name="alpha"/>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	    <eq>
	      <type/>
	      <minz>
		<type/>
	      </minz>
	    </eq>
	    <neq>
	      <type/>
	      <tvar name="alpha"/>
	      <tvar name="beta"/>
	    </neq>
	    <notin>
	      <Tsub>
		<tvar name="alpha"/>
		<type/>
	      </Tsub>
	      <aCtset/>
	    </notin>
	  </lhs>
	  <rhs>
	    <Pair>
	      <aCtset num="i"/>
	      <scomp>
		<aSubMap num="a"/>
		<aSubMap num="i"/>
	      </scomp>	    
	    </Pair>	  	    
	    <eq>
	      <aSubMap num="a"/>
	      <SubMap>
		<tvar name="alpha"/>
		<type/>
	      </SubMap>
	    </eq>
	    <eq>
	      <unf cl="yes">
		<Subst>
		  <aSubMap num="a"/>
		  <aCtset/>
		</Subst>
	      </unf>
	      <Pair>
		<aCtset num="i"/>
		<aSubMap num="i"/>
	      </Pair>
	    </eq>
	  </rhs>
	</equation>	
	<!-- t1->t2 <= t1' -> t2' -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <fn>		    
		      <type num="a"/>		    		    
		      <type num="r"/>		    
		    </fn>
		    <fn>		    
		      <type num="a" dash="'"/>		    		    
		      <type num="r" dash="'"/>
		    </fn>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf cl="yes">
	      <unin>
		<aCtset/>
		<set>
		  <eq>
		    <type num="a"/>
		    <type num="a" dash="'"/>
		  </eq>
		  <eq>
		    <type num="r"/>
		    <type num="r" dash="'"/>
		  </eq>
		</set>
	      </unin>
	    </unf>
	  </rhs>
	</equation>	
	<!-- ^t1 <= ^t2 -->
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <ref>
		      <type num="1"/>
		    </ref>
		    <ref>
		      <type num="2"/>
		    </ref>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf cl="yes">
	      <unin>
		<aCtset/>
		<set>
		  <eq>
		    <type num="1"/>
		    <type num="2"/>
		  </eq>
		</set>
	      </unin>
	    </unf>
	  </rhs>
	</equation>	
	<!-- Mt1 <= Mt2 -->
	<equation sep="4pt">
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Tsub>
		    <mutable>
		      <type num="1"/>
		    </mutable>
		    <mutable>
		      <type num="2"/>
		    </mutable>
		  </Tsub>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf cl="yes">
	      <unin>
		<aCtset/>
		<set>
		  <eq>
		    <type num="1"/>
		    <type num="2"/>
		  </eq>
		</set>
	      </unin>
	    </unf>
	  </rhs>
	</equation>	
	<!-- P(m, t1, t2) --> 
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Pcst>
		    <lKind k="mono"/>
		    <type num="g"/>
		    <type num="i"/>
		  </Pcst>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	  </lhs>
	  <rhs>
	    <unf cl="yes">
	      <unin>
		<aCtset/>
		<set>
		  <eq>
		    <type num="g"/>
		    <type num="i"/>
		  </eq>
		</set>
	      </unin>
	    </unf>
	  </rhs>
	</equation>	
	<!-- P(p, t1, t2) --> 
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<set>
		  <Pcst>
		    <lKind k="poly"/>
		    <type num="g"/>
		    <type num="i"/>
		  </Pcst>
		</set>
		<aCtset/>
	      </unin>
	    </unf>
	    <pred name="Immut">
	      <type num="i"/>
	    </pred>
	    <eq>
	      <fnxn name="dtv">
		<type num="i"/>
		<Empty/>
	      </fnxn>
	    </eq>
	  </lhs>
	  <rhs>
	    <unf>
	      <aCtset/>
	    </unf>
	  </rhs>
	</equation>	
	<!-- P(k, Mt1?, M?t2) --> 
	<equation>
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<aCtset num="*"/>
		<aCtset/>
	      </unin>
	    </unf>
	    <eq>
	      <aCtset num="*"/>
	      <set>
		<Pcst>
		  <lKind k="var"/>
		  <type num="g"/>
		  <type num="i"/>
		</Pcst>
	      </set>
	    </eq>
	    <pred name="Mut">
	      <type num="i"/>
	    </pred>
	  </lhs>
	  <rhs>
	    <Pair>
	      <aCtset num="i"/>
	      <scomp>
		<aSubMap num="k"/>
		<aSubMap num="i"/>
	      </scomp>
	    </Pair>
	    <eq>
	      <aSubMap num="k"/>
	      <SubMap>
		<lKind k="var"/>
		<lKind k="mono"/>
	      </SubMap>
	    </eq>
	    <eq>
	      <unf>
		<Subst>
		  <aSubMap num="k"/>
		  <aCtset num="*"/>
		</Subst>
	      </unf>
	      <Pair>
		<aCtset num="i"/>
		<aSubMap num="i"/>
	      </Pair>
	    </eq>
	  </rhs>
	</equation>	
	<!-- P(k, t, t1), P(k, t, t2)  t1 !~ t2 -->
	<equation sep="4pt">
	  <lhs>
	    <unf ac="yes">
	      <unin>
		<aCtset num="*"/>
		<aCtset/>
	      </unin>
	    </unf>
	    <eq>
	      <aCtset num="*"/>
	      <set>
		<Pcst>
		  <lKind k="var"/>
		  <type/>
		  <type num="1"/>
		</Pcst>
		<Pcst>
		  <lKind k="var"/>
		  <type/>
		  <type num="2"/>
		</Pcst>
	      </set>
	    </eq>
	    <eq>
	      <unf cl="yes">
		<set>
		  <unin>
		    <eq>		    
		      <type num="1"/>
		      <type num="2"/>
		    </eq>
		    <aCtset/>
		  </unin>
		</set>
	      </unf>
	      <error/>
	    </eq>
	  </lhs>
	  <rhs>
	    <Pair>
	      <aCtset num="i"/>
	      <scomp>
		<aSubMap num="k"/>
		<aSubMap num="i"/>
	      </scomp>
	    </Pair>
	    <eq>
	      <aSubMap num="k"/>
	      <SubMap>
		<lKind k="var"/>
		<lKind k="poly"/>
	      </SubMap>
	    </eq>
	    <eq>
	      <unf>
		<Subst>
		  <aSubMap num="k"/>
		  <aCtset num="*"/>
		</Subst>
	      </unf>
	      <Pair>
		<aCtset num="i"/>
		<aSubMap num="i"/>
	      </Pair>
	    </eq>
	  </rhs>
	</equation>	
	<!-- Error (Cyclic) -->
	<equation>
	  <lhs>
	    <unf>
	      <aCtset/>
	    </unf>
	    <pred name="cyclic">
	      <aCtset/>
	    </pred>
	  </lhs>
	  <rhs>
	    <error/>
	  </rhs>
	</equation>
	<!-- Error (unhandled, un-unifiable) -->
	<equation>
	  <lhs>
	    <unf>
	      <unin>
		<constraint/>
		<aCtset/>
	      </unin>
	    </unf>
	    <notin>
	      <constraint/>
	      <unin>
		<aCtset num="rem"/>
		<aCtset num="hdl"/>
	      </unin>
	    </notin>
	  </lhs>
	  <rhs>
	    <error/>
	  </rhs>
	</equation>
      </VEqns>      
      <VEqns>
	<equation>
	  <lhs>	    
	    <aCtset num="rem"/>
	  </lhs>
	  <rhs>
	    <grouping>
	      <Forall>
		<collection>
		  <tvar name="alpha"/>
		  <lKind k="var"/>
		  <type/>
		  <type dash="'"/>
		</collection>
	      </Forall>
	      <textit content="."/>
	      <space/>
	      <set>
		<Tsub>
		  <tvar name="alpha"/>
		  <type/>
		</Tsub>
		<Tsub>
		  <type/>
		  <tvar name="alpha"/>
		</Tsub>
		<Pcst>
		  <lKind k="var"/>
		  <type/>
		  <type dash="'"/>
		</Pcst>
	      </set>
	    </grouping>
	  </rhs>
	</equation>
	<equation>
	  <lhs>
	    <aCtset num="hdl"/>
	  </lhs>
	  <rhs>
	    <grouping>
	      <Forall>
		<collection>
		  <tvar name="alpha"/>
		  <lKind k="var"/>
		  <lKind/>
		  <type/>
		  <type dash="'"/>
		  <type num="1"/>
		  <type num="1" dash="'"/>
		  <type num="i"/>
		</collection>
	      </Forall>
	      <text content="|"/>
	      <pred name="Immut">
		<type num="i"/>
	      </pred>
	      <space/>
	      <text content="."/>
	      <space/>	      
	      <set>
		<Tsub>
		  <type/>
		  <type/>
		</Tsub>
		<Tsub>
		  <mutable>
		    <type/>
		  </mutable>
		  <type/>
		</Tsub>
		<eq>
		  <lKind k="var"/>
		  <lKind/>
		</eq>
		<Tsub>
		  <fn>
		    <type/>
		    <type num="1"/>
		  </fn>
		  <fn>
		    <type dash="'"/>
		    <type num="1" dash="'"/>
		  </fn>
		</Tsub>
		<Tsub>
		  <ref>
		    <type/>
		  </ref>
		  <ref>
		    <type dash="'"/>
		  </ref>
		</Tsub>
		<Tsub>
		  <mutable>
		    <type/>
		  </mutable>
		  <mutable>
		    <type dash="'"/>
		  </mutable>
		</Tsub>
		<Pcst>
		  <lKind k="mono"/>
		  <type/>
		  <type dash="'"/>
		</Pcst>
		<Pcst>
		  <lKind k="poly"/>
		  <type/>
		  <type num="i"/>
		</Pcst>
	      </set>
	    </grouping>
	  </rhs>
	</equation>	
       </VEqns>
    </btypes:TYPE>
  </font>
  <caption>Equational Unification Rules</caption>
</figure>
