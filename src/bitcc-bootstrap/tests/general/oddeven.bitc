(bitc-version "0.9+")

(define oe (letrec ((odd1 (lambda (x) (even1 x))) 
		    (even1 (lambda (x) (odd1 x)))) (odd1, even1))) 


(define (odd2, even2) ((lambda (x) (even2 x)), 
 		       (lambda (x) (odd2 x)))) 

