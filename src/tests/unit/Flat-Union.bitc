bitc version 0.11
import bitc.main as main
provide bitc.main main

unboxed union unin
is C1
   C2 is c21:int32
         c22:bool
   C3

instance Eq(unin) 
is _==_ = lambda (x, y) true
   _!=_ = lambda (x, y) false

(def main.main:(fn (vector string) -> int32)
  (lambda (argvec) 
    (let ((n:(mutable int32) 10))
      C1 (C2 10 true)
      (== C1 (C2 n true))
      (== C3 C1)
      0)))

