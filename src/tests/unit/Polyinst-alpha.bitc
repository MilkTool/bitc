(bitc-version "0.10")
(import bitc.main as main)
(provide bitc.main main)

(define i0 (the int32 0))
(define i1 (the int32 1))

(define (f) (letrec ((lam (lambda (x) x))) i0))
(define main.main:(fn (vector string) -> int32)
  (lambda (argvec) (f) i0))
