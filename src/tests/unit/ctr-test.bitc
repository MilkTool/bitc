(bitc-version "0.10")
(import main bitc.main)
(provide bitc.main main)

(defunion (unin 'a 'b) (C1 c1:'a c11:'b) (C2 c2:'a))
 
(define value (C2 0:int32)) 
  
(define bVal value.C2)
(define main.main:(fn ((vector string)) int32)
  (lambda (argvec)
    (if bVal value.C1 value.C2) 0))

