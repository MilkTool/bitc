(bitc version 0.10)
(import bitc.main as main)
(provide bitc.main main)

(defstruct (pr1 'a 'b):val
  (fill (bitfield int32 16))
  a:'a
  b:'b)

(defstruct (pr2 'a 'b):val
  a:'a
  (reserved (bitfield int32 16) 16)
  b:'b)

(defrepr repr:val
  (Ct1  p:(pr1 bool int32) i:int32 j:bool (where (== i 0)))
  (Ct2  q:(pr2 int32 bool)  i:int32 (where (== i 1))))
 
(define main.main:(fn (vector string) -> int32)
  (lambda (argvec) 
    (Ct1 (pr1 #t 10) #f) 0))
